{
  "expo": {
    "name": "Vidoo",
    "slug": "Vidoo",
    "version": "1.0.0",
       "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "vidoo",
    "userInterfaceStyle": "light",
    // "newArchEnabled": true,
    
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
      // "bundleIdentifier": "com.kamilfr.vidoo"
    },
    "android": {
      "jsEngine": "hermes",
      
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kamilfr.vidoo"
    },
    "web": {
      // "bundler": "metro",
      // "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
